<div class="text-right">
  <%= link_to  "Add New List", new_list_path, class:"btn btn-primary btn_add" %>
</div>
<div class="row">
  <% @lists.each do |l|%>
  <div class = " col-sm-12 col-md-6 col-lg-4">
     <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= image_url(l.imagen) %>')">
      <%= link_to l.name, list_path(l), class: "link"%>
    </div>
</div>
<% end %>

